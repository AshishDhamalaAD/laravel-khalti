{
    "name": "asdh/laravel-khalti",
    "description": "A laravel package to verify khalti payment.",
    "keywords": [
        "khalti",
        "payment gateway",
        "khalti payment gateway",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashish Dhamala",
            "email": "ashish.dhamala2015@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php" : "^7.2.5"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "AsDh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsDh\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AsDh\\KhaltiServiceProvider"
            ]
        }
    }
}